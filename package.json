{
  "name": "idcard",
  "version": "1.0.0",
  "engines": {
    "npm": "6.1.0",
    "node": "10.4.1"
  },
  "description": "UW WebServices Registration POC",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --inspect --config nodemon.json",
    "prebuild": "rm -rf dist",
    "build": "cross-env NODE_ENV=production babel src/backend -s -D -d dist/backend && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "start": "node dist/backend",
    "test": "eslint src && cross-env NODE_ENV=test mocha --require babel-core/register test/*.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwwebservices/idcard-webapp-poc.git"
  },
  "author": "UWWebServices <ews-team@uw.edu>",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "browser-cookies": "^1.2.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "csv-express": "^1.2.2",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-saml": "^0.33.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-fontawesome": "^1.6.1",
    "react-notification-system": "^0.2.17",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "mocha": "^5.2.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.10",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "sass-loader": "^6.0.7",
    "should": "^13.2.3",
    "style-loader": "^0.21.0",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
