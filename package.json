{
  "name": "uwer",
  "version": "1.0.0",
  "engines": {
    "npm": "6.14.4",
    "node": "14.11.0"
  },
  "description": "UW WebServices Registration POC",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --inspect --config nodemon.json",
    "prebuild": "rm -rf dist",
    "build": "cross-env NODE_ENV=production babel src/backend -s -D -d dist/backend && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "start": "node dist/backend",
    "test": "eslint src && cross-env NODE_ENV=test mocha --require babel-core/register test/*.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwwebservices/idcard-webapp-poc.git"
  },
  "author": "UWWebServices <ews-team@uw.edu>",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "csv-express": "^1.2.2",
    "ews-api-lib": "git+https://github.com/uwwebservices/ews-api-lib.git#0.2.8",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphite": "^0.1.4",
    "helmet": "^3.15.1",
    "memorystore": "^1.6.2",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-saml": "^0.33.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-notification-system": "^0.4.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "response-time": "^2.3.2",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.11.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.10",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass-loader": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}